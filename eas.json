{
    "cli": {
      "version": "^16.3.3",
      "appVersionSource": "remote"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal",
        "ios": {
          "resourceClass": "m1-medium"
        }
      },
      "production": {
        "android": {
          "buildType": "app-bundle"
        },
        "ios": {
          "resourceClass": "m1-medium",
          "simulator": false,
          "distribution": "store"
        }
      }
    },
    "submit": {
      "production": {
        "ios": {
          "appleId": "${APPLE_ID}",
          "ascAppId": "${APPLE_APP_ID}",
          "appleTeamId": "${APPLE_TEAM_ID}"
        }
      }
    }
  }
